{
  "name": "babel-plugin-html-tag",
  "version": "1.0.7",
  "description": "Statically evaluates and minify tagged html`<..>` template literals into strings",
  "main": "index.js",
  "engines": {
    "node": ">=8.6"
  },
  "scripts": {
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinovyatkin/babel-plugin-html-tag.git"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "babel",
    "babel-plugin",
    "html",
    "template-literals"
  ],
  "author": "Tino Vyatkin <tino@vtkn.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tinovyatkin/babel-plugin-html-tag/issues"
  },
  "homepage": "https://github.com/tinovyatkin/babel-plugin-html-tag#readme",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@destinationstransfers/eslint-config": "latest",
    "eslint": "latest",
    "eslint-plugin-node": "latest",
    "jest": "latest"
  },
  "directories": {
    "test": "__tests__"
  },
  "jest": {
    "collectCoverage": true,
    "testEnvironment": "node",
    "verbose": true
  },
  "eslintConfig": {
    "plugins": [
      "node"
    ],
    "extends": [
      "@destinationstransfers",
      "plugin:node/recommended"
    ],
    "env": {
      "node": true
    },
    "parserOptions": {
      "sourceType": "script"
    }
  },
  "dependencies": {
    "html-minifier": "^3.5.21"
  }
